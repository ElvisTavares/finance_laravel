{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "asgrim/ofxparser": "1.2.2",
        "barryvdh/laravel-translation-manager": "^0.4.2",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "^6.3",
        "jeremykenedy/laravel-roles": "^1.2",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "laravelnews/laravel-twbs4": "^1.3",
        "php-http/guzzle6-adapter": "2.0.1",
        "sentry/sentry-laravel": "1.0.2"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/helpers.php",
            "app/Base/ApplicationModel.php",
            "app/Base/ApplicationController.php",
            "app/Helpers/Account/Period.php",
            "app/Helpers/Invoice/Invoice.php",
            "app/Helpers/Account/Formatter.php",
            "app/Helpers/Account/Values.php",
            "app/Helpers/Upload/CSV.php",
            "app/Helpers/Upload/OFX.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
